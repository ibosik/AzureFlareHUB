    end)

local DisabledToggle = Section.NewToggle("FOV",function(bool)
if bool == true then
		game:GetService("TweenService"):Create(workspace.Camera, TweenInfo.new(1, Enum.EasingStyle.Quint), {FieldOfView = 120}):Play()
else
		game:GetService("TweenService"):Create(workspace.Camera, TweenInfo.new(1, Enum.EasingStyle.Quint), {FieldOfView = 70}):Play()
end
end,false)

