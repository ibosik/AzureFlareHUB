local Tab = Window.NewTab("Auto Buy")

local Section = Tab.NewSection("Turtle")

local Button = Section.NewButton("Auto Buy Turtle",function()
while wait() do
    local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage").IncomeEvent:FireServer(unpack(args))
        end
end)

local Button = Section.NewButton("Auto Buy MegaTurtle",function()
