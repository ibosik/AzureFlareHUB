    Section:NewToggle("Jesus (walk on water)", "WALK ON WATER!!!", function(state)
        if state then
            for _,v in pairs(game.Workspace.Water:GetChildren()) do
                v.CanCollide = true
                end
        else
            for _,v in pairs(game.Workspace.Water:GetChildren()) do
                v.CanCollide = false
                end
        end
    end)


