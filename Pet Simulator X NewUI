for i,v in pairs(getgc(true)) do
   if (typeof(v) == 'table' and rawget(v, 'OpenEgg')) then
       v.OpenEgg = func
		func = nil
        end
    end
for i,v in pairs(getgc(true)) do
   if (typeof(v) == 'table' and rawget(v, 'OpenEgg')) then
       func = v.OpenEgg
       v.OpenEgg = function()
           return true
   end
   end
   end
   end
   end
end)
